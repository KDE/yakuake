#, kde-format
msgctxt "@option:check"
msgid "Keep window open when it loses focus"
msgstr "窗口失去焦点时保持展开"

#. i18n: ectx: property (text), widget (QCheckBox, kcfg_KeepOpenAfterLastSessionCloses)
#: app/config/behaviorsettings.ui:141
#, kde-format
msgctxt "@option:check"
